{
  "service/druid-metrics": {
    "input": {
      "timestamp": {
        "column": "timestamp",
        "format": "iso"
      },
      "camus": [
        "/hdfs/location1",
        "/hdfs/location2"
      ]
    },
    "output": {
      "segmentGranularity": "minute",
      "indexGranularity": "minute",
      "numShards": 1
    },
    "metrics": {
      "value": "doubleSum"
    },
    "dimensions": [
      "host",
      "metric",
      "service",
      "user1",
      "user2"
    ]
  }
}
